Blockly.Blocks['area'] = {
  init: function() {
    this.setHelpUrl('http://www.example.com/');
    this.setColour(20);
    this.appendValueInput("AREA")
        .setCheck("Number")
        .appendField("Area");
    this.appendDummyInput()
        .appendField("In")
        .appendField(new Blockly.FieldDropdown([["square feet", "SQUARE_FEET"], ["square miles", "SQUARE_MILES"], ["option", "OPTIONNAME"]]), "NAME");
    this.setOutput(true, "Number");
    this.setTooltip('');
  }
};


